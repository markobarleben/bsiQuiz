<ul class="list-group fa-ul">
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-calendar fa-lg"></i>
    <%- currentDay %>
  </li>
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-user-circle-o fa-lg"></i>
    <%- user %>
  </li>
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-bullseye fa-lg"></i>
    <%- sumOfMaxQuestionPoints %>
  </li>
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-check-square fa-lg"></i>
    <%- points %>
  </li>
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-percent fa-lg"></i>
    <%- percent %>
  </li>
  <li class="list-group-item col-xs-12"><i class="fa-li fa fa-trophy fa-lg"></i>Nicht Bestanden</li>
</ul>
<div class="col-lg-12 col-md-12 col-sm-12 col-sx-12" style="margin-top:2em; margin-bottom:2em;" >
  <button type="button" class="btn btn-default btn-sm" id="returnToFirstQuestion">Neuer Versuch</button>
</div>